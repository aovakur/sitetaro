def authorization_verification(f):
    @wraps(f)
    def decorator_function(*args, **kwargs):
        try:
            if userprofile.id == null:          
                abort(403)
        except AttributeError: 
            abort(403)

        return f(*args, **kwargs)
    return decorator_function

